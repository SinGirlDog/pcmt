   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">A.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">建设工程勘察合同</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">B.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">委托合同</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">C.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">建筑工程施工合同</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">D.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">承揽合同</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">E.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">建设工程设计合同</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s71"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">【答案】</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">ACE</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s71"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">【解析】本题考查的是建设工程施工合同的法定形式。《合同法》规定，建设工程合同应当采用书面形式。参考教材</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">P106.</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">63.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">根据《合同法》，违约方承担赔偿损失的构成要件包括（）</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">A.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">具有违约行为</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">B.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">具有惩罚目的</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">C.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">造成损失后果</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">D.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">违约方有过错，或虽无过错，但法律规定应当赔偿</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">E.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">违约行为与损失后果之间具有因果关系</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s71"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">【答案】</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">ACDE</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s71"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">【解析】本题考查的是承担赔偿损失责任的构成要件。（</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">1</Font><Font
       html:Color="#000000">）具有违约行为；（</Font><Font html:Face="Times New Roman"
       x:Family="Roman" html:Color="#000000">2</Font><Font html:Color="#000000">）造成损失后果；（</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">3</Font><Font
       html:Color="#000000">）违约行为与财产等损失之间有因果关系；（</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">4</Font><Font
       html:Color="#000000">）违约人有过错，或者虽无过错，但法律规定应当赔偿。参见教材</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">P113.</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">64</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">．根据《合同法》，合同权利义务终止的情形有（</Font><Font
       html:Color="#000000"> </Font><Font html:Face="宋体" x:CharSet="134"
       html:Color="#000000">）。</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">A</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">．债务人依法将标的物抵押</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">B</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">．合同解除</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">C</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">．债务相互抵消</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">D</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">．债权人依法将标的物提存</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">E</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">．债权人免除债务</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s71"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">【答案】</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">BCE</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="27">
    <Cell ss:MergeAcross="18" ss:StyleID="s71"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">【解析】本题考查的是合同的终止。《合同法》规定，有下列情形之一的，合同的权利义务终止：（</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">1</Font><Font
       html:Color="#000000">）债务已经按照约定履行；（</Font><Font html:Face="Times New Roman"
       x:Family="Roman" html:Color="#000000">2</Font><Font html:Color="#000000">）合同解除；（</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">3</Font><Font
       html:Color="#000000">）债务互相抵消；（</Font><Font html:Face="Times New Roman"
       x:Family="Roman" html:Color="#000000">4</Font><Font html:Color="#000000">）债务人依法将标的物提存；（</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">5</Font><Font
       html:Color="#000000">）债权人免除责任；（</Font><Font html:Face="Times New Roman"
       x:Family="Roman" html:Color="#000000">6</Font><Font html:Color="#000000">）债权债务同归于一人；（</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">7</Font><Font
       html:Color="#000000">）法律规定或者当事人约定终止的其他情形。参见教材</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">P121.</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">65.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">根据《建筑工程质量管理条例》，建设工程竣工验收应当具备的条件有（</Font><Font
       html:Color="#000000"> </Font><Font html:Face="宋体" x:CharSet="134"
       html:Color="#000000">）</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">A.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">完成建设工程设计的各项内容</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">B.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">有完整的技术档案和施工管理资料</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">C.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">有工程使用的主要建筑材料，建筑构配件和设备的进场试验报告</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">D.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">有监理单位和施工单位共同签署的质量合格文件</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s68"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">E.</Font><Font
       html:Face="宋体" x:CharSet="134" html:Color="#000000">有建设单位签署的工程保修书</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="18" ss:StyleID="s71"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">【答案】</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">BC</Font></ss:Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="27">
    <Cell ss:MergeAcross="18" ss:StyleID="s71"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40"><Font html:Color="#000000">【解析】本题考查的是竣工验收应当具备的法定条件。选项</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">A</Font><Font
       html:Color="#000000">不太全面：完成建设工程设计和合同约定的各项内容；选项</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">D</Font><Font
       html:Color="#000000">：有勘察、设计、施工、工程监理等单位分别签署的质量合格文件；选项</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">E</Font><Font
       html:Color="#000000">：有施工单位签署的工程保修书。参见教材</Font><Font
       html:Face="Times New Roman" x:Family="Roman" html:Color="#000000">P278</Font><Font
       html:Color="#000000">。</Font></ss:Data></Cell>
   </Row>
  </Table>
